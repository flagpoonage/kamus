{
  "manifest_version": 3,
  "name": "Kamus",
  "description": "An extension to manage your trees of notes",
  "version": "1.0",
  "action": {
    "default_icon": {
      "16": "icons/icon16.png",
      "24": "icons/icon24.png",
      "32": "icons/icon32.png",
      "48": "icons/icon48.png"
    },
    "default_title": "Kamus",
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "icons/icon16.png",
    "24": "icons/icon24.png",
    "32": "icons/icon32.png",
    "48": "icons/icon48.png",
    "96": "icons/icon96.png",
    "128": "icons/icon128-96wb.png"
  },
  "background": {
    "service_worker": "background.js"
  },
  "permissions": ["contextMenus", "storage"]
}
